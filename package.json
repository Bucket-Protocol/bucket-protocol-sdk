{
  "name": "@bucket-protocol/sdk",
  "author": "Bucket Protocol <info@bucketprotocol.io>",
  "version": "1.0.3",
  "license": "Apache-2.0",
  "description": "Bucket Protocol TypeScript SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bucket-Protocol/bucket-protocol-sdk.git"
  },
  "homepage": "https://github.com/Bucket-Protocol/bucket-protocol-sdk#readme",
  "bugs": {
    "url": "https://github.com/Bucket-Protocol/bucket-protocol-sdk/issues"
  },
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "lint": "eslint",
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "build:types": "tsc -b ./tsconfig.types.json",
    "build:cjs": "tsc -b ./tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "build:esm": "tsc -b ./tsconfig.esm.json && echo '{\"type\": \"module\"}' > dist/esm/package.json",
    "build": "pnpm clean && npm-run-all --parallel build:types build:cjs build:esm",
    "test": "vitest"
  },
  "dependencies": {
    "@mysten/sui": "1.38.0",
    "@mysten/bcs": "1.8.0",
    "@pythnetwork/pyth-sui-js": "^2.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "@changesets/cli": "^2.26.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "prettier": "^3.6.2",
    "typescript": "5.9.2",
    "vitest": "^0.34.1"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "resolutions": {
    "@mysten/sui": "1.38.0",
    "@mysten/bcs": "1.8.0"
  },
  "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706"
}
